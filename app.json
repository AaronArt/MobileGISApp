{
  "expo": {
    "name": "mobile_gis",
    "slug": "mobile_gis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
        "config": {
          "googleMaps": {
            "apiKey": "AIzaSyDi7GzLT9GeT5uI9PKFlRKCiMaUfj5iUBs"
          }
      }
    }
    
    ,
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
